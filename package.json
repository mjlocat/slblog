{
  "name": "slblog",
  "version": "1.0.0",
  "description": "ServerLess Blog",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "start-install": "node install/install.js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjlocat/slblog.git"
  },
  "author": "Michael Locatelli",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/mjlocat/slblog/issues"
  },
  "homepage": "https://github.com/mjlocat/slblog#readme",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "mock-fs": "^5.4.1"
  },
  "dependencies": {
    "@aws-sdk/client-route-53": "^3.716.0",
    "prompts": "^2.4.2",
    "winston": "^3.17.0"
  }
}
